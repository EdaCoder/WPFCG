<UserControl
    x:Class="WPFCG.UserCtrl.HandwareInfo"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WPFCG.UserCtrl"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    Loaded="InitEvent"
    mc:Ignorable="d">
    <StackPanel>
        <StackPanel Margin="20" Orientation="Horizontal">
            <TextBlock Style="{DynamicResource Handware}" Text="CPU型号" />
            <TextBlock
                x:Name="C1"
                Margin="20,0"
                Style="{DynamicResource Handware}"
                Text="Intel Core" />
        </StackPanel>
        <StackPanel Margin="20" Orientation="Horizontal">
            <TextBlock Style="{DynamicResource Handware}" Text="主板型号" />
            <TextBlock
                x:Name="C2"
                Margin="20,0"
                Style="{DynamicResource Handware}"
                Text="Intel Core" />
        </StackPanel>
        <StackPanel Margin="20" Orientation="Horizontal">
            <TextBlock Style="{DynamicResource Handware}" Text="内存型号" />
            <TextBlock
                x:Name="C3"
                Margin="20,0"
                Style="{DynamicResource Handware}"
                Text="Intel Core" />
        </StackPanel>
        <StackPanel Margin="20" Orientation="Horizontal">
            <TextBlock Style="{DynamicResource Handware}" Text="硬盘型号" />
            <TextBlock
                x:Name="C4"
                Margin="20,0"
                Style="{DynamicResource Handware}"
                Text="Intel Core" />
        </StackPanel>
        <StackPanel Margin="20" Orientation="Horizontal">
            <TextBlock Style="{DynamicResource Handware}" Text="显卡型号" />
            <TextBlock
                x:Name="C5"
                Margin="20,0"
                Style="{DynamicResource Handware}"
                Text="Intel Core" />
        </StackPanel>
        <StackPanel Margin="20" Orientation="Horizontal">
            <TextBlock Style="{DynamicResource Handware}" Text="声卡型号" />
            <TextBlock
                x:Name="C6"
                Margin="20,0"
                Style="{DynamicResource Handware}"
                Text="Intel Core" />
        </StackPanel>
        <StackPanel Margin="20" Orientation="Horizontal">
            <TextBlock Style="{DynamicResource Handware}" Text="网卡型号" />
            <TextBlock
                x:Name="C7"
                Margin="20,0"
                Style="{DynamicResource Handware}"
                Text="Intel Core" />
        </StackPanel>
        <StackPanel Margin="20" Orientation="Horizontal">
            <TextBlock Style="{DynamicResource Handware}" Text="系统版本" />
            <TextBlock
                x:Name="C8"
                Margin="20,0"
                Style="{DynamicResource Handware}"
                Text="Intel Core" />
        </StackPanel>
    </StackPanel>
</UserControl>
